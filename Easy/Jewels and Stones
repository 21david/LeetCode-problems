*/

class Solution {
    
    public int numJewelsInStones2(String J, String S) {
        // faster than 70.51%
        
        int count = 0;
        
        for(char c : S.toCharArray())
            if(J.indexOf(c) >= 0)
                count++;
        
        return count;
    }
}
